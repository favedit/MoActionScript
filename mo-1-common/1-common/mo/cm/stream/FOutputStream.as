package mo.cm.stream
{
   import flash.utils.ByteArray;
   
   //============================================================
   // <T>数据输出流。</T>
   //============================================================
   public class FOutputStream extends FOutput
   {
      //============================================================
      // <T>构造数据输出流。</T>
      //============================================================
      public function FOutputStream(){
         super(new ByteArray());
      }
      
      //============================================================
      // <T>释放资源。</T>
      //============================================================
      public override function dispose():void{
         memory.clear();
         super.dispose();
      }
   }
}