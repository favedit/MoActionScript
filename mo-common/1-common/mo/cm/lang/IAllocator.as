package mo.cm.lang
{
   //============================================================
   // <T>收集器接口。</T>
   //============================================================
   public interface IAllocator
   {
      //============================================================
      // <T>根据类型创建对象实例。</T>
      //
      // @param p:type 类型
      // @return 对象实例
      //============================================================
      function create(p:String):*;
   
      //============================================================
      // <T>释放一个对象。</T>
      //
      // @param p:object 对象
      //============================================================
      function free(p:*):void;
   }
}